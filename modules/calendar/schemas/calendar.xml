<table name="calendar">
    <column name="id" type="bigtext"/>
    <column name="calendarId" type="number" size="11" notnull="notnull"/>
    <column name="title" type="varchar" size="250" />
    <column name="body" type="text"/>
    <column name="start" type="text"/>
    <column name="end" type="text"/>
    <column name="goingDuration" type="number" size="11" />
    <column name="comingDuration" type="number" size="11" />
    <column name="isAllDay" type="number" size="1" />
    <column name="category" type="varchar" size="8"/>
    <column name="dueDateClass" type="varchar" size="8"/>
    <column name="location" type="text" />
    <column name="recurrenceRule" type="char" size="1"/>
    <column name="isPending" type="char" size="1"/>
    <column name="isFocused" type="char" size="1"/>
    <column name="isVisible" type="char" size="1"/>
    <column name="isReadOnly" type="char" size="1"/>
    <column name="isPrivate" type="char" size="1"/>
    <column name="color" type="varchar" size="8" />
    <column name="bgColor" type="varchar" size="8"/>
    <column name="dragBgColor" type="varchar" size="8"/>
    <column name="borderColor" type="varchar" size="8"/>
    <column name="customStyle" type="varchar" size="8"/>
    <column name="raw" type="bigtext"/>
    <column name="state" type="text"/>
</table>
