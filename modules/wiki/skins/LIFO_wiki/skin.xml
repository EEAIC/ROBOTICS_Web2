<?xml version="1.0" encoding="UTF-8"?>
<skin version="0.2">
    <title xml:lang="ko">LIFO Wiki Skin</title>
	<title xml:lang="en">LFIO Wiki Skin</title>
    <description xml:lang="ko">
        LIFO Wiki Skin
    </description>
	<description xml:lang="en">
        This is LIFO skin of Wiki
    </description>
    <version>0.1</version>
    <date>2019-02-22</date>
    <author email_address="artherlim@gmail.com" link="https://gconmaster.github.io">
        <name xml:lang="ko">JEONGKEE LIM</name>
		<name xml:lang="vi">JEONGKEE LIM</name>
        <name xml:lang="en">JEONGKEE LIM</name>
        <name xml:lang="jp">JEONGKEE LIM</name>
		<name xml:lang="zh-CN">JEONGKEE LIM</name>
        <name xml:lang="zh-TW">JEONGKEE LIM</name>
        <name xml:lang="tr">JEONGKEE LIM</name>
    </author>
    <license>LGPL</license>
    <extra_vars>
        <var name="use_bootstrapCDN" type="select" default="N">
            <title xml:lang="ko">BootstrapCDN 사용</title>
            <title xml:lang="en">Use BootstrapCDN  </title>
            <description xml:lang="ko">부트스트랩의 CSS 와 자바스크립트를 CDN 으로 지원합니다.</description>
            <description xml:lang="en">Use BootstrapCDN, provided for free by the folks at StackPath.</description>
            <options value="Y">
                <title xml:lang="ko">사용함</title>
                <title xml:lang="en">Y</title>
            </options>
            <options value="N">
                <title xml:lang="ko">사용 안 함</title>
                <title xml:lang="en">N</title>
            </options>
        </var>
        <var name="use_gfm" type="select" default="Y">
            <title xml:lang="ko">github flavored markdown 사용</title>
            <title xml:lang="en">Use github flavored markdown</title>
            <description xml:lang="ko">깃허브의 마크다운 디자인을 일부 지원합니다.</description>
            <description xml:lang="en">Support github-markdown partly</description>
            <options value="Y">
                <title xml:lang="ko">사용함</title>
                <title xml:lang="en">Y</title>
            </options>
            <options value="N">
                <title xml:lang="ko">사용 안 함</title>
                <title xml:lang="en">N</title>
            </options>
        </var>
        <var name="nav_bgcolor" type="text" default="#008275">
            <title xml:lang="ko">메뉴바 색상</title>
            <title xml:lang="en">Navbar Color</title>
            <description xml:lang="ko">메뉴바 색상을 변경할 수 있습니다.</description>
            <description xml:lang="en">Change Navbar Color</description>

        </var>
        <var name="menu_style" type="select">
            <title xml:lang="ko">Tree Menu style</title>
			<title xml:lang="en">Tree Menu style</title>
            <description xml:lang="ko">Please choose the style of tree menu.</description>
			<description xml:lang="en">Please choose the style of tree menu.</description>
			<options value="msdn">
                <title xml:lang="ko">MSDN style tree</title>
				<title xml:lang="en">MSDN style tree</title>
			</options>		
			<options value="classic">
                <title xml:lang="ko">Classic tree</title>
				<title xml:lang="en">Classic tree</title>
			</options>
            <options value="toast_ui_grid">
                <title xml:lang="ko">TOAST UI GRID</title>
				<title xml:lang="en">TOAST UI GRID</title>
			</options>
		</var>

         <var name="wiki_logo" type="image">
			<title xml:lang="ko">로고</title>
            <title xml:lang="en">Logo</title>
			<title xml:lang="vi">Tiêu đề</title>
            <title xml:lang="jp">タイトル</title>
            <title xml:lang="zh-CN">제목</title>
            <title xml:lang="zh-TW">標題</title>
            <title xml:lang="tr">Başlık</title>
            <description xml:lang="ko">로고 이미지를 사용하시려면 등록해주세요.</description>
			<description xml:lang="en">Please input the logo.</description>
        </var>

        <var name="main_title" type="text">
			<title xml:lang="ko">제목</title>
            <title xml:lang="en">Title</title>
			<title xml:lang="vi">Tiêu đề</title>
            <title xml:lang="jp">タイトル</title>
            <title xml:lang="zh-CN">제목</title>
            <title xml:lang="zh-TW">標題</title>
            <title xml:lang="tr">Başlık</title>
            <description xml:lang="ko">제목을 적어주세요.</description>
			<description xml:lang="en">Plase input the title.</description>
			<description xml:lang="vi">Xin hãy nhập tiêu đề.</description>
			<description xml:lang="jp">タイトルを入力して下さい。</description>
            <description xml:lang="zh-CN">제목을 적어주세요.</description>
            <description xml:lang="zh-TW">請輸入標題。</description>
            <description xml:lang="tr">Lütfen başlığı giriniz.</description>
        </var>
        

        
		<var name="comment" type="textarea">
			<title xml:lang="ko">상세 설명</title>
            <title xml:lang="en">Description</title>
            <title xml:lang="vi">Mô tả</title>
			<title xml:lang="jp">詳細</title>
            <title xml:lang="zh-CN">상세 설명</title>
            <title xml:lang="zh-TW">說明</title>
            <title xml:lang="tr">Tanım</title>
            <description xml:lang="ko">게시판 제목 아래 표시될 설명을 입력하실 수 있습니다.</description>
			<description xml:lang="en">You may input description which will be displayed under the title.</description>
            <description xml:lang="vi">Xin hãy nhập mô tả cho Wiki, nó sẽ được hiển thị phía dưới tiêu đề.</description>
			<description xml:lang="jp">タイトルの下に表示される説明文を入力して下さい。</description>
            <description xml:lang="zh-CN">게시판 제목 아래 표시될 설명을 입력하실 수 있습니다.</description>
            <description xml:lang="zh-TW">請輸入顯示在標題下方的說明。</description>
            <description xml:lang="tr">Başlığın altında görüntülenecek bir tanım girebilirsiniz.</description>
        </var>
    
	</extra_vars>
</skin>

